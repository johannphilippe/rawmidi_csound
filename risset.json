      "extractpath": "jsfx.dll",
      "csound_version": ">=6.17<7.0"
    },
    {
      "platform": "windows",
      "url": "https://github.com/csound-plugins/csound-plugins/releases/download/v${version}/csound7-plugins--win64.zip",
      "extractpath": "jsfx.dll",
      "csound_version": ">=7.0"
    }
