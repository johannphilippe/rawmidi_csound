{
  "name": "rawmidi",
  "version": "0.0.1",
  "opcodes": [
    "rawmidi_in_open",
    "rawmidi_out_open",
    "rawmidi_in",
    "rawmidi_out",
    "sysex_print"
  ],
  "libname": "librawmidi",
  "short_description": "RtMidi support for raw MIDI in Csound",
  "long_description": "A raw MIDI opcode for Csound allowing using MIDI without filters",
  "author": "Johann Philippe",
  "email": "johannphilippe@lilo.org",
  "license": "LGPL",
  "repository": "https://github.com/johannphilippe/rawmidi_csound",
