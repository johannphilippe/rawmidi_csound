      "csound_version": ">=6.17<7.0"
    },
    {
      "platform": "macos",
      "url": "https://github.com/csound-plugins/csound-plugins/releases/download/v${version}/csound7-plugins--macos.zip",
      "extractpath": "libjsfx.dylib",
      "csound_version": ">=7.0"
    },
    {
